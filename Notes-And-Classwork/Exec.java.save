public class Exec{
	public String box(int r,int c){
		String RIVEN = new String("");
		while (RIVEN.length() < c) {
			RIVEN = RIVEN + "#";
		}
		int i = 0;
		while (i < r) {
			RIVEN = RIVEN + "\n" + RIVEN;
		}
		return RIVEN;
	}
	public static void main(String[] args){
		System.out.println(Exec.box(4,3));
	}
}
